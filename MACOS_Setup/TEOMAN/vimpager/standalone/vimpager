macros_less_vim() {
	(cat <<'EOF') | do_uudecode > macros/less.vim
begin 644 macros/less.vim
M(B(@5FEM('-C<FEP="!T;R!W;W)K(&QI:V4@(FQE<W,B"B(@36%I;G1A:6YE
